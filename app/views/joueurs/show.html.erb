<p class="notice"><%= notice %></p>

<div class="joueur-show">
  <%= render @joueur %>
</div>

<div class="actions">
  <%= link_to "Edit this joueur", edit_joueur_path(@joueur), class: "btn btn-warning" %> |
  <%= link_to "Back to joueurs", joueurs_path, class: "btn btn-secondary" %>
  <%= button_to "Destroy this joueur", @joueur, method: :delete, class: "btn btn-danger" %>
</div>
